div
  input#show-unimplemented(type="checkbox",
    onchange="show_unimplemented(this.checked)")
  label Show unsupported codes

  script
    | function show_unimplemented(show) {
    |   var elems = document.querySelectorAll(".unimplemented");
    |   for (var i = 0; i < elems.length; i++)
    |     elems[i].style.visibility = show ? 'visible' : 'collapse';
    | }
    | show_unimplemented(document.querySelector('#show-unimplemented').checked);
